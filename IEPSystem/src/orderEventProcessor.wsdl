<definitions targetNamespace="orderEventProcessor_iep"
             xmlns:tns="orderEventProcessor_iep"
             xmlns:typens="orderEventProcessor_iep"
             xmlns:defns="orderEventProcessor_iep"
             xmlns:xsd="http://www.w3.org/2001/XMLSchema"
             xmlns:soap="http://schemas.xmlsoap.org/wsdl/soap/"
             xmlns:file="http://schemas.sun.com/jbi/wsdl-extensions/file/"
             xmlns="http://schemas.xmlsoap.org/wsdl/">
<types>
    <xsd:schema targetNamespace="orderEventProcessor_iep" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
        <xsd:element name="OrderInput_MsgObj">
            <xsd:complexType>
                <xsd:sequence>
                    <xsd:element name="sellerId" type="xsd:int"/>
                    <xsd:element name="expectPrice" type="xsd:double"/>
                    <xsd:element name="price" type="xsd:double"/>
                </xsd:sequence>
            </xsd:complexType>
        </xsd:element>
        <xsd:element name="OrderInputBatch_MsgObj">
            <xsd:complexType>
                <xsd:sequence>
                    <xsd:element name="OrderInput_MsgObj" minOccurs="0" maxOccurs="unbounded">
                        <xsd:complexType>
                            <xsd:sequence>
                    <xsd:element name="sellerId" type="xsd:int"/>
                    <xsd:element name="expectPrice" type="xsd:double"/>
                    <xsd:element name="price" type="xsd:double"/>
                            </xsd:sequence>
                        </xsd:complexType>
                    </xsd:element>
                </xsd:sequence>
            </xsd:complexType>
        </xsd:element>
        <xsd:element name="StreamOutput0_MsgObj">
            <xsd:complexType>
                <xsd:sequence>
                   <xsd:element name="sellerId" type="xsd:int"/>
                   <xsd:element name="expectPrice" type="xsd:double"/>
                   <xsd:element name="price" type="xsd:double"/>
                </xsd:sequence>
            </xsd:complexType>
        </xsd:element>
    </xsd:schema>
</types>

<message name="OrderInput_Msg">
    <part name="input" element="typens:OrderInput_MsgObj"/>
</message>
<message name="OrderInputBatch_Msg">
    <part name="input" element="typens:OrderInputBatch_MsgObj"/>
</message>
<message name="StreamOutput0_Msg">
    <part name="output" element="typens:StreamOutput0_MsgObj"/>
</message>

<portType name="InputPt">
    <operation name="OrderInput">
        <input message="tns:OrderInput_Msg"/>
    </operation>
    <operation name="OrderInputBatch">
        <input message="tns:OrderInputBatch_Msg"/>
    </operation>
</portType>
<portType name="OutputPt_StreamOutput0">
    <operation name="StreamOutput0">
        <input message="tns:StreamOutput0_Msg"/>
    </operation>
</portType>

<plnk:partnerLinkType name="InputPlt" xmlns:plnk="http://docs.oasis-open.org/wsbpel/2.0/plnktype">
    <plnk:role name = "InputRn" portType="tns:InputPt"/>
</plnk:partnerLinkType>
<plnk:partnerLinkType name="OutputPlt_StreamOutput0" xmlns:plnk="http://docs.oasis-open.org/wsbpel/2.0/plnktype">
    <plnk:role name = "OutputRn_StreamOutput0" portType="tns:OutputPt_StreamOutput0"/>
</plnk:partnerLinkType>
<!-- input binding -->
<binding name="InputBinding" type="defns:InputPt">
    <soap:binding style="document" transport="http://schemas.xmlsoap.org/soap/http"/>
    <operation name="OrderInput">
        <soap:operation soapAction="OrderInput"/>
        <input>
            <soap:body use="literal"/>
        </input>
    </operation>
    <operation name="OrderInputBatch">
        <soap:operation soapAction="OrderInputBatch"/>
        <input>
            <soap:body use="literal"/>
        </input>
    </operation>
</binding>
<!-- input service -->
<service name="InputService">
    <port name="InputPort" binding="tns:InputBinding">
        <soap:address location="http://localhost:12100/service/orderEventProcessor_iep"/>
    </port>
</service>

<!-- http output binding and service
<binding name="OutputBinding_StreamOutput0" type="defns:OutputPt_StreamOutput0">
    <operation name="StreamOutput0">
        <soap:operation soapAction="StreamOutput0"/>
        <input>
            <soap:body use="literal"/>
        </input>
    </operation>
</binding>
<service name="OutputService_StreamOutput0">
    <port name="OutputPort_StreamOutput0" binding="tns:OutputBinding_StreamOutput0">
        <soap:address location="http://localhost:12100/service/orderEventProcessor_iep_StreamOutput0_callee"/>
    </port>
</service>
 end of http output binding and service -->

<!-- file output binding and service -->
<binding name="OutputBinding_StreamOutput0" type="defns:OutputPt_StreamOutput0">
    <file:binding/>
    <operation name="StreamOutput0">
        <file:operation/>
        <input>
            <file:message fileName="StreamOutput0.txt"
                    fileNameIsPattern="false"
                    addEOL="false"
                    multipleRecordsPerFile="true"
                    use="literal"/>
        </input>
    </operation>
</binding>
<service name="OutputService_StreamOutput0">
    <port name="OutputPort_StreamOutput0" binding="tns:OutputBinding_StreamOutput0">
        <file:address fileDirectory="C:/temp/orderEventProcessor_iep"/>
    </port>
</service>
<!-- end of file output binding and service -->
</definitions>
